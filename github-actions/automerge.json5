{
  $schema: "https://docs.renovatebot.com/renovate-schema.json",
  description: "github-actions: automerge non major versions",
  packageRules: [
    {
      matchManagers: ["github-actions"],
      automerge: true,
      matchUpdateTypes: ["minor", "patch", "digest"],
      automergeType: "pr",
    },
  ],
}
